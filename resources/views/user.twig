<!doctype html>
<head>
    <title>Список пользователей</title>
</head>
<body>
<h2>Список пользователей</h2>
<p>Вывод через twig</p>
<table border="2">
    {% for user in users %}
    <tr>
        <td>{{ user.first_name }}</td>
        <td>{{ user.email }}</td>
    </tr>
    {% endfor %}
</table>
</body>
</html>
